{
  "extends": "@voiceflow/tsconfig",
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "lib": ["dom", "es2019"],
    "jsx": "react-jsx",
    "baseUrl": ".",
    "outDir": "build",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["node"]
  },
  "exclude": ["node_modules", "build", "dist", "docs"]
}
